#include<stdio.h>
#include<conio.h>
int main()
{
    int inttype;
    float floattype;
    char chartype;
    double doubletype;
    printf("Size of int %d bytes \n",sizeof inttype);
    printf("Size of float %d bytes \n",sizeof floattype);
    printf("Size of char %d bytes \n",sizeof chartype);
    printf("Size of double %d bytes \n",sizeof doubletype);
    return 0;

}
